# EasyTest 本地开发环境配置示例

FLASK_ENV=development
FLASK_APP=wsgi.py
SECRET_KEY=dev-secret-key-for-testing
JWT_SECRET_KEY=dev-jwt-secret-key-for-testing

# 使用 PostgreSQL 数据库
DATABASE_URL=postgresql+psycopg2://easytest:easytest123@127.0.0.1:5432/easytest_dev

# Redis 配置
REDIS_URL=redis://localhost:6379/0

# Celery 配置
CELERY_ENABLE=true

#跳过playwright安装
KIP_PLAYWRIGHT_BROWSERS=1

# 报告存储路径
REPORT_FOLDER=./reports
UPLOAD_FOLDER=./uploads
